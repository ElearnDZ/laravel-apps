{
    "name": "elfsundae/laravel-apps",
    "type": "library",
    "description": "Laravel multi applications support.",
    "keywords": ["laravel", "multi", "apps", "domain"],
    "homepage": "https://github.com/ElfSundae/laravel-apps",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.0,<5.6"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.4|~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\Laravel\\Apps\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\Laravel\\Apps\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ElfSundae\\Laravel\\Apps\\AppsServiceProvider"
            ]
        }
    }
}
